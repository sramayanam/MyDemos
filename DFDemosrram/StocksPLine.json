{
  "name": "StocksPLine",
  "properties": {
    "description": "Enrich Stocks Data",
    "activities": [
      {
        "type": "HDInsightHive",
        "typeProperties": {
          "scriptPath": "scripts/partition.hql",
          "scriptLinkedService": "StorageLinkedServiceinput",
          "defines": {
            "inputtable": "wasb://stocks@storagefordemodfsrram.blob.core.windows.net/",
            "partitionedtable": "wasb://patitioned@storagefordemodfsrram.blob.core.windows.net/data/",
            "Year": "$$Text.Format('{0:yyyy}',SliceStart)",
            "Month": "$$Text.Format('{0:%M}',SliceStart)"
          }
        },
        "inputs": [
          {
            "name": "stocks-input"
          }
        ],
        "outputs": [
          {
            "name": "stocks-partitioned"
          }
        ],
        "scheduler": {
          "frequency": "Month",
          "interval": 1
        },
        "name": "PartitionData",
        "linkedServiceName": "HDInsightLinkedService"
      },
      {
        "type": "HDInsightHive",
        "typeProperties": {
          "scriptPath": "scripts/transform.hql",
          "scriptLinkedService": "StorageLinkedServiceinput",
          "defines": {
            "stocksdataInput": "$$Text.Format('wasb://patitioned@storagefordemodfsrram.blob.core.windows.net/data/yearno={0:yyyy}/monthno={0:%M}/',SliceStart)",
            "refdataInput": "wasb://refdata@storagefordemodfsrram.blob.core.windows.net/",
            "stocksOutput": "wasb://stocksenriched@storagefordemodfsrram.blob.core.windows.net/stocks/"
          }
        },
        "inputs": [
          {
            "name": "stocks-partitioned"
          },
          {
            "name": "ref-data"
          }
        ],
        "outputs": [
          {
            "name": "stocks-enriched"
          }
        ],
        "policy": {
          "timeout": "01:00:00",
          "concurrency": 3,
          "executionPriorityOrder": "NewestFirst",
          "retry": 1
        },
        "scheduler": {
          "frequency": "Month",
          "interval": 1
        },
        "name": "JoinData",
        "description": "Join Enrich stocks data",
        "linkedServiceName": "HDInsightLinkedService"
      },
      {
        "type": "Copy",
        "typeProperties": {
          "source": {
            "type": "BlobSource"
          },
          "sink": {
            "type": "SqlSink",
            "writeBatchSize": 0,
            "writeBatchTimeout": "00:00:00"
          }
        },
        "inputs": [
          {
            "name": "stocks-enriched"
          }
        ],
        "outputs": [
          {
            "name": "Stocks-SQLTable"
          }
        ],
        "policy": {
          "timeout": "01:00:00",
          "concurrency": 1,
          "executionPriorityOrder": "NewestFirst",
          "style": "StartOfInterval"
        },
        "scheduler": {
          "frequency": "Month",
          "interval": 1
        },
        "name": "BlobtoSQLLoad",
        "description": "Copy Activity"
      },
      {
        "type": "SqlServerStoredProcedure",
        "typeProperties": {
          "storedProcedureName": "sp_stocks",
          "storedProcedureParameters": {
            "dateTime": "$$Text.Format('{0:yyyy-MM-dd HH:mm:ss}', SliceStart)",
            "pipelinename": "StocksPipeline"
          }
        },
        "inputs": [
          {
            "name": "Stocks-SQLTable"
          }
        ],
        "outputs": [
          {
            "name": "Stocks-Final"
          }
        ],
        "scheduler": {
          "frequency": "Month",
          "interval": 1
        },
        "name": "Add ETL Audits"
      }
    ],
    "start": "2015-02-01T00:00:00Z",
    "end": "2015-02-28T00:00:00Z",
    "isPaused": false,
    "pipelineMode": "Scheduled"
  },
  "$schema": "http://datafactories.schema.management.azure.com/schemas/2015-08-01/Microsoft.DataFactory.Pipeline.json"
}